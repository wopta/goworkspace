				{
					"name": "init",
					"inputData": [
						{
							"name": "config",
							"type": "callbackConfig"
						}
					],
					"handlerless": true,
					"gateways": [
						{
							"nextActivities": [
								"end"
							],
							"decision": "!config.emit"
						},
						{
							"nextActivities": [
								"winEmit"
							],
							"decision": "node.callbackConfig.name==`winClient`"
						},
						{
							"nextActivities": [
								"baseCallback"
							],
							"decision": "node.callbackConfig.name==`facileBrokerClient`"
						}
					]
				},
				{
					"name": "baseCallback",
					"outputData": [
						{
							"name": "callbackInfo",
							"type": "callbackInfo"
						}
					],
					"gateways": [
						{
							"nextActivities": [
								"saveAudit"
							]
						}
					]
				},
				{
					"name": "winEmit",
